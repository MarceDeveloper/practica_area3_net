# Get espacios (requires auth)
GET {{SREO.API_HostAddress}}/espacios
Authorization: Bearer YOUR_TOKEN_HERE

###

# Create espacio (requires auth)
POST {{SREO.API_HostAddress}}/espacios
Content-Type: application/json
Authorization: Bearer YOUR_TOKEN_HERE

{
  "nombre": "Sala de Reuniones A",
  "tipo": "SalaReuniones",
  "capacidad": 10,
  "caracteristicas": "Proyector, WiFi",
  "disponible": true
}

###

# Get reservas (requires auth)
GET {{SREO.API_HostAddress}}/reservas
Authorization: Bearer YOUR_TOKEN_HERE

